<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="flintstones.domain.ui">
      <domain_ui
            chart="jfreechart.LinguisticDomainChart"
            domain="flintstones.domain.linguistic.unbalanced"
            icon="icons/Letter-L-icon_16.png">
      </domain_ui>
   </extension>
   <extension
         point="flintstones.domain.ui.new.dialog">
      <dialog
            description="Create unbalanced domain"
            domain="flintstones.domain.linguistic.unbalanced"
            id="sinbad2.domain.linguistic.newU"
            implementation="sinbad2.domain.linguistic.unbalanced.ui.dialog.LHDomainDialog">
      </dialog>
   </extension>
   <extension
         point="flintstones.domain.ui.modify.dialog">
      <dialog
            description="Modify unbalanced domain"
            domain="flintstones.domain.linguistic.unbalanced"
            id="sinbad2.domain.linguistic.modifyU"
            implementation="sinbad2.domain.linguistic.unbalanced.ui.dialog.LHModifyDomainDialog">
      </dialog>
   </extension>

</plugin>
