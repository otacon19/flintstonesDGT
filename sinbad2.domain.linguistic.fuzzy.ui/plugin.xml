<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="flintstones.domain.ui">
      <domain_ui
            chart="jfreechart.LinguisticDomainChart"
            domain="flintstones.domain.linguistic"
            icon="icons/Letter-L-icon_16.png">
      </domain_ui>
   </extension>
   <extension
         point="flintstones.domain.ui.new.dialog">
      <dialog
            description="Create auto-generated domain"
            domain="flintstones.domain.linguistic"
            id="sinbad2.domain.linguistic.newauto"
            implementation="dialog.AutoGenerateLinguisticDomainDialog">
      </dialog>
      <dialog
            description="Manually create domain"
            domain="flintstones.domain.linguistic"
            id="sinbad2.domain.linguistic.newmanual"
            implementation="dialog.ManualLinguisticDomainDialog">
      </dialog>
   </extension>
   <extension
         point="flintstones.domain.ui.modify.dialog">
      <dialog
            description="Manually modify dialog"
            domain="flintstones.domain.linguistic"
            id="sinbad2.domain.linguistic.modify"
            implementation="dialog.ModifyLinguisticDomainDialog">
      </dialog>
   </extension>

</plugin>
