   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="sinbad2.element.ui.handler.expert.add.AddExpertHandler"
            commandId="flintstones.element.expert.add">
      </handler>
      <handler
            class="sinbad2.element.ui.handler.expert.modify.ModifyExpertHandler"
            commandId="flintstones.element.expert.modify">
         <enabledWhen>
            <and>
               <with
                     variable="selection">
                  <iterate
                        operator="and">
                     <instanceof
                           value="sinbad2.element.expert.Expert">
                     </instanceof>
                  </iterate>
               </with>
               <reference
                     definitionId="oneElementSelected">
               </reference>
            </and>
         </enabledWhen>
      </handler>
      <handler
            class="sinbad2.element.ui.handler.expert.remove.RemoveExpertHandler"
            commandId="flintstones.element.expert.remove">
         <activeWhen>
            <and>
               <with
                     variable="selection">
                  <iterate
                        ifEmpty="false"
                        operator="and">
                     <instanceof
                           value="sinbad2.element.expert.Expert">
                     </instanceof>
                  </iterate>
               </with>
               <reference
                     definitionId="oneElementSelected">
               </reference>
            </and>
         </activeWhen>
      </handler>
      <handler
            class="sinbad2.element.ui.handler.expert.remove.RemoveMultiplesExpertsHandler"
            commandId="flintstones.element.expert.remove">
         <activeWhen>
            <and>
               <with
                     variable="selection">
                  <iterate
                        ifEmpty="false"
                        operator="and">
                     <instanceof
                           value="sinbad2.element.expert.Expert">
                     </instanceof>
                  </iterate>
               </with>
               <not>
                  <reference
                        definitionId="oneElementSelected">
                  </reference>
               </not>
            </and>
         </activeWhen>
      </handler>
      <handler
            class="sinbad2.element.ui.handler.alternative.add.AddAlternativeHandler"
            commandId="flintstones.element.alternative.add">
      </handler>
      <handler
            class="sinbad2.element.ui.handler.alternative.modify.ModifyAlternativeHandler"
            commandId="flintstones.element.alternative.modify">
         <enabledWhen>
            <and>
               <with
                     variable="selection">
                  <iterate>
                     <instanceof
                           value="sinbad2.element.alternative.Alternative">
                     </instanceof>
                  </iterate>
               </with>
               <reference
                     definitionId="oneElementSelected">
               </reference>
            </and>
         </enabledWhen>
      </handler>
      <handler
            class="sinbad2.element.ui.handler.alternative.remove.RemoveAlternativeHandler"
            commandId="flintstones.element.alternative.remove">
         <activeWhen>
            <and>
               <with
                     variable="selection">
                  <iterate
                        ifEmpty="false"
                        operator="and">
                     <instanceof
                           value="sinbad2.element.alternative.Alternative">
                     </instanceof>
                  </iterate>
               </with>
               <reference
                     definitionId="oneElementSelected">
               </reference>
            </and>
         </activeWhen>
      </handler>
      <handler
            class="sinbad2.element.ui.handler.alternative.remove.RemoveMultipleAlternativeHandler"
            commandId="flintstones.element.alternative.remove">
         <activeWhen>
            <and>
               <with
                     variable="selection">
                  <iterate
                        ifEmpty="false"
                        operator="and">
                     <instanceof
                           value="sinbad2.element.alternative.Alternative">
                     </instanceof>
                  </iterate>
               </with>
               <not>
                  <reference
                        definitionId="oneElementSelected">
                  </reference>
               </not>
            </and>
         </activeWhen>
      </handler>
      <handler
            class="sinbad2.element.ui.handler.criterion.add.AddCriterionHandler"
            commandId="flintstones.element.criterion.add">
      </handler>
      <handler
            class="sinbad2.element.ui.handler.criterion.modify.ModifyCriterionHandler"
            commandId="flintstones.element.criterion.modify">
         <enabledWhen>
            <and>
               <with
                     variable="selection">
                  <iterate>
                     <instanceof
                           value="sinbad2.element.criterion.Criterion">
                     </instanceof>
                  </iterate>
               </with>
               <reference
                     definitionId="oneElementSelected">
               </reference>
            </and>
         </enabledWhen>
      </handler>
      <handler
            class="sinbad2.element.ui.handler.criterion.remove.RemoveCriterionHandler"
            commandId="flintstones.element.criterion.remove">
         <activeWhen>
            <and>
               <with
                     variable="selection">
                  <iterate
                        ifEmpty="false"
                        operator="and">
                     <instanceof
                           value="sinbad2.element.criterion.Criterion">
                     </instanceof>
                  </iterate>
               </with>
               <reference
                     definitionId="oneElementSelected">
               </reference>
            </and>
         </activeWhen>
      </handler>
      <handler
            class="sinbad2.element.ui.handler.criterion.remove.RemoveMultipleCriteriaHandler"
            commandId="flintstones.element.criterion.remove">
         <activeWhen>
            <and>
               <with
                     variable="selection">
                  <iterate
                        ifEmpty="false"
                        operator="and">
                     <instanceof
                           value="sinbad2.element.criterion.Criterion">
                     </instanceof>
                  </iterate>
               </with>
               <not>
                  <reference
                        definitionId="oneElementSelected">
                  </reference></not>
            </and>
         </activeWhen>
      </handler>
